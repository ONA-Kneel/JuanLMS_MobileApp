{
  "expo": {
    "name": "JuanLMS_MobileApp",
    "slug": "JuanLMS_MobileApp",
    "owner": "BossKneel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/JuanLMS-LogoV1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/JuanLMS-LogoV1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.juanlms.mobileapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/JuanLMS-LogoV1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.juanlms.mobileapp",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "softwareKeyboardLayoutMode": "pan",
      "allowBackup": true,
      "enableProguardInReleaseBuilds": true,
      "enableShrinkResources": true,
      "enableR8": true
    },
    "web": {
      "icon": "./assets/JuanLMS-LogoV1.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you take profile pictures."
        }
      ],
      "@react-native-community/datetimepicker"
    ]
  }
}
